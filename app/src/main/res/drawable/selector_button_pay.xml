<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:fitsSystemWindows="true">
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/px34"/>
            <solid android:color="#e9e9e9"/>
            <stroke
                android:width="1dp"
                android:color="#ededed"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/px34"/>
            <solid android:color="#ffffff"/>
            <stroke
                android:width="1dp"
                android:color="#ededed"/>
        </shape>
    </item>
</selector>

<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:fitsSystemWindows="true">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/px75"/>
            <solid android:color="#afd3e8"/>
            <stroke
                android:width="@dimen/px5"
                android:color="@color/st_goodname"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/px75"/>
            <solid android:color="#fff"/>
            <stroke
                android:width="@dimen/px5"
                android:color="@color/st_goodname"/>
        </shape>
    </item>
</selector>

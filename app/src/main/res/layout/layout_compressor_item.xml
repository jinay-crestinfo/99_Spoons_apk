<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@drawable/bank_bg"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:textSize="@dimen/text_xsmall"
        android:textColor="@color/color_text"
        android:gravity="center"
        android:id="@+id/tv_index"
        android:background="@color/color_index"
        android:layout_width="@dimen/cell_item_width"
        android:layout_height="25px"/>
    <View
        android:id="@+id/line01"
        android:background="@color/color_line"
        android:layout_width="@dimen/cell_item_width"
        android:layout_height="1px"
        android:layout_below="@+id/tv_index"/>
    <EditText
        android:textSize="@dimen/text_small"
        android:background="@drawable/grid_item_bg"
        android:layout_width="@dimen/cell_item_width"
        android:layout_height="@dimen/new_setting_item_height"
        android:singleLine="true"
        android:layout_below="@+id/tv_index"/>
    <View
        android:id="@+id/line02"
        android:background="@color/color_line"
        android:layout_width="@dimen/cell_item_width"
        android:layout_height="1px"
        android:layout_below="@+id/tv_index"/>
    <EditText
        android:textSize="@dimen/text_small"
        android:background="@drawable/grid_item_bg"
        android:layout_width="@dimen/cell_item_width"
        android:layout_height="@dimen/new_setting_item_height"
        android:singleLine="true"
        android:layout_below="@+id/tv_index"/>
    <View
        android:id="@+id/line03"
        android:background="@color/color_line"
        android:layout_width="@dimen/cell_item_width"
        android:layout_height="1px"
        android:layout_below="@+id/tv_index"/>
    <EditText
        android:textSize="@dimen/text_small"
        android:background="@drawable/grid_item_bg"
        android:layout_width="@dimen/cell_item_width"
        android:layout_height="@dimen/new_setting_item_height"
        android:singleLine="true"
        android:layout_below="@+id/tv_index"/>
</LinearLayout>
